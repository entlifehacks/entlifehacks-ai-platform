# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Application URL (your custom domain)
VITE_APP_URL=https://entlifehacks.com

# SendGrid Email Configuration
VITE_SENDGRID_API_KEY=SG.your-sendgrid-api-key-here

# Admin Configuration
VITE_ADMIN_EMAIL=admin@entlifehacks.com
VITE_ADMIN_PASSWORD=your-secure-admin-password

# Calendly Integration
VITE_CALENDLY_URL=https://calendly.com/eclh-consulting/consultation

# Analytics (Optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_PLAUSIBLE_DOMAIN=entlifehacks.com

# Feature Flags (Optional)
VITE_ENABLE_AB_TESTING=true
VITE_ENABLE_EMAIL_ANALYTICS=true

# API Rate Limiting (Optional)
VITE_API_RATE_LIMIT=100
VITE_API_RATE_WINDOW=900000
